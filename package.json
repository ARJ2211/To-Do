{
    "name": "todo",
    "version": "1.0.0",
    "description": "A simple task manager :)",
    "main": "index.js",
    "scripts": {
        "electron": "electron .",
        "dist": "build",
        "build-installer": "electron-builder"
    },
    "build": {
        "appId": "ToDo",
        "win": {
            "target": [
                "nsis"
            ]
        },
        "nsis": {
            "uninstallDisplayName": "todo",
            "oneClick": false,
            "allowToChangeInstallationDirectory": true
        }
    },
    "author": "Aayush Jadhav",
    "license": "MIT",
    "dependencies": {
        "custom-electron-titlebar": "^3.2.6",
        "electron-packager": "^15.2.0",
        "electron-store": "^6.0.1",
        "path": "^0.12.7"
    },
    "devDependencies": {
        "custom-electron-titlebar": "^3.2.6",
        "electron": "^11.1.1",
        "electron-builder": "^22.9.1"
    }
}
